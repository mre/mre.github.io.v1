---
layout: nil
name:   "http://www.matthias-endler.de"
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset
    xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
<url>
  <loc>{{ page.name }}/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>1.0</priority>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>{{ page.name }}/about/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.5</priority>
  <changefreq>weekly</changefreq>
</url>
<url>
  <loc>{{ page.name }}/movies/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.5</priority>
  <changefreq>weekly</changefreq>
</url>
<url>
  <loc>{{ page.name }}/projects/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.5</priority>
  <changefreq>weekly</changefreq>
</url>
<url>
  <loc>{{ page.name }}/page2/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.1</priority>
  <changefreq>weekly</changefreq>
</url>
<url>
  <loc>{{ page.name }}/page3/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.1</priority>
  <changefreq>weekly</changefreq>
</url>
<url>
  <loc>{{ page.name }}/page4/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.1</priority>
  <changefreq>weekly</changefreq>
</url>
<url>
  <loc>{{ page.name }}/page5/</loc>
  <lastmod>{{ site.time | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.1</priority>
  <changefreq>weekly</changefreq>
</url>
{% for post in site.posts %}
<url>
  <loc>{{ page.name }}{{ post.url }}</loc>
  <lastmod>{{ post.date | date:"%Y-%m-%d" }}</lastmod>
  <priority>0.5</priority>
  <changefreq>weekly</changefreq>
</url>
{% endfor %}
</urlset>
