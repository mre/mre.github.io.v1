---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Matthias Endler</title>
 <link href="http://www.matthias-endler.de/atom.xml" rel="self"/>
 <link href="http://www.matthias-endler.de/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.matthais-endler.de/</id>
 <author>
   <name>Matthias Endler</name>
   <email>matthias-endler@gmx.net</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.matthias-endler.de{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.matthias-endler.de{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>